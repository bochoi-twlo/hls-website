# description: This application name. Also used as name of used Twilio service SID/KEY
# format: text
# required: true
# configurable: false
# default: hls-website
APPLICATION_NAME=hls-website

# description: Twilio phone number to send/receive SMS from. Your customer will see this number.
# format: phone_number
# required: true
# configurable: true
TWILIO_PHONE_NUMBER=

# description: Customer name to be displayed
# format: text
# required: true
# configurable: true
# default: OwlHealth
CUSTOMER_NAME=OwlHealth

# description: Set a password for your admnistration page.
# format: secret
# required: true
ADMINISTRATOR_PASSWORD=

# description: Change the salt to invalidate existing auth tokens.
# format: secret
# required: true
# configurable: false
# default: salty
SALT=salty

# description: Administrator phone number to receive security code for 2nd factor authentication.
# format: phone_number
# required: true
# configurable: true
ADMINISTRATOR_PHONE=

# description: Administrator email to receive information.
# format: email
# required: true
# configurable: true
ADMINISTRATOR_EMAIL=

# description: Twilio serverless service SID. Created automatically on deployment with friendlyName = APPLICATION_NAME.
# format: text
# required: true
# configurable: false
TWILIO_SERVICE_SID=

# description: Twilio Flex service instance SID.
# format: text
# required: true
# configurable: false
FLEX_SID=

# description: Twilio flow SID for channelType=web (aka webchat)
# format: text
# required: true
# configurable: false
FLEX_WEB_FLOW_SID=

# description: Twilio task router workspace SID
# format: text
# required: true
# configurable: false
FLEX_WORKSPACE_SID=
